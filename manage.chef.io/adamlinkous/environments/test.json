      "firewalld": {
        "permanent": true
      }
    },
    "mywrapper_chef-slack_handler": {
      "webhooks": {
        "list": [
          "family_test_notify"
        ]
      }
    },
    "chef_client": {
      "handler": {
        "slack": {
          "fail_only": false,
          "send_start_message": true
        }
      }
    }
  }
