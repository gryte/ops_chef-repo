{
  "name": "prod",
  "description": "prod",
  "cookbook_versions": {
    "delugeserver": "~> 2.2.0",
    "7dtdserver": "= 1.2.0",
    "jenkinsserver": "= 1.0.0",
    "dockerserver": "~> 1.4.0",
    "plexserver": "~> 0.8.0",
    "manage_nfs": "~> 0.2.0",
    "mywrapper_chef_client_updater": "~> 0.8.0",
    "mywrapper_chef-client": "~> 0.2.0",
    "mywrapper_chef-slack_handler": "~> 0.2.0"
  },
  "json_class": "Chef::Environment",
  "chef_type": "environment",
  "default_attributes": {

  },
  "override_attributes": {
    "7dtdserver": {
      "serversettings": {
        "servername": "7dtdserver_prod",
        "servermaxplayercount": "3",
        "serverdescription": "Adam's Private Server",
        "gameworld": "Random Gen",
        "gamename": "Forge the Narrative",
        "controlpanelenabled": "true",
        "controlpanelport": "8083"
      }
    },
    "firewall": {
      "firewalld": {
        "permanent": true
      }
    },
    "mywrapper_chef-slack_handler": {
      "webhooks": {
        "list": [
          "family_notify"
        ]
      }
    }
  }
}
