{
  "name": "test",
  "description": "",
  "cookbook_versions": {
    "delugeserver": "= 1.4.0",
    "jenkinsserver": "= 0.1.0",
    "7dtdserver": "= 1.2.0",
    "dockerserver": "= 0.1.0",
    "plexserver": "= 0.7.1"
  },
  "json_class": "Chef::Environment",
  "chef_type": "environment",
  "default_attributes": {

  },
  "override_attributes": {
    "id": "custom_override",
    "deluge": {
      "config": {
        "auth": true,
        "core.conf": {
          "manage": true,
          "settings": {
            "move_completed_path": "/.deluge/complete",
            "move_completed": "true",
            "allow_remote": "true",
            "download_location": "/.deluge/staging",
            "utpex": "false",
            "dht": "false",
            "lsd": "false",
            "random_port": "false"
          }
        },
        "label.conf": true
      },
      "plugin": {
        "enable": [
          "AutoAdd",
          "Label"
        ]
      },
      "app": {
        "directories": {
          "/.deluge": {
            "owner": "deluge",
            "group": "deluge"
          },
          "/.deluge/staging": {
            "owner": "deluge",
            "group": "deluge"
          },
          "/.deluge/prep": {
            "owner": "deluge",
            "group": "deluge"
          },
          "/.deluge/complete": {
            "owner": "deluge",
            "group": "deluge"
          },
          "/.deluge/complete/tv": {
            "owner": "deluge",
            "group": "deluge"
          },
          "/.deluge/complete/movie": {
            "owner": "deluge",
            "group": "deluge"
          }
        }
      }
    },
    "7dtdserver": {
      "serversettings": {
        "servername": "7dtdserver_test",
        "servermaxplayercount": "2",
        "serverdescription": "Adam's Private Server",
        "gameworld": "Random Gen",
        "gamename": "Forge the Narrative",
        "controlpanelenabled": "true",
        "controlpanelport": "8083"
      }
    }
  }
}
